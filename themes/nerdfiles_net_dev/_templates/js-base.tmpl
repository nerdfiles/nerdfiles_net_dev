{% block script %}
<script src='//code.jquery.com/jquery.min.js'></script>
<!--script src='{{ ASSETS_URL }}js-lib/jquery-waypoints/waypoints.min.js'></script-->
<!--script src='{{ ASSETS_URL }}js-lib/metaquery/metaquery.min.js'></script-->
<script>
(function ($) {
    $('body').data('SITE_URL', '{{ SITE_URL }}');
    $('html').removeClass('no-js');
    $('li').on('touchstart', function () {
        var that = this;
        window.setTimeout(function () {
            $(that).find('.exo').addClass('hover');
        }, 1000);
        window.setTimeout(function () {
            $(that).find('.exo').addClass('hover');
        }, 4000);
    });
})(jQuery);
</script>
<script src='{{ ASSETS_URL }}js/{{ COFFEE_URL }}/main.js'></script> 
