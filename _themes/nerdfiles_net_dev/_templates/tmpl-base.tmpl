{% load cms_tags menu_tags sekizai_tags %}

<!DOCTYPE HTML>

{% block font %}
{% addtoblock "font" %}
{% include "font-base.tmpl" %}
{% endaddtoblock %}
{% endblock %}

{% block css %}
{% addtoblock "css" %}
{% include "css-base.tmpl" %}
{% endaddtoblock %}
{% endblock %}

{% block scriptloader %}
{% addtoblock "scriptloader" %}
{% include "js-scriptloader.tmpl" %}
{% endaddtoblock %}
{% endblock %}

{% block js %}
{% addtoblock "js" %}
{% include "js-base.tmpl" %}
{% endaddtoblock %}
{% endblock %}

{% block analytics %}
{% addtoblock "analytics" %}
{% include "js-analytics.tmpl" %}
{% endaddtoblock %}
{% endblock %}

<html lang="en">

<head>

<meta charset="utf-8" />

<title>/-;</title>

{% render_block "font" %}
{% render_block "css" %}
{% render_block "scriptloader" %}

</head>

<body class="">

{% cms_toolbar %}

<div class="container">
{% block main_content %}
  {% placeholder "Main Content" or %}
    
    <p>blugggh.</p>

    {% get_lastfm_widget as lastfm_widget %}
    <!--h2>{{ lastfm_widget.title }}</h2-->
    <div class="mod mod-lastfm">
    	{{ lastfm_widget.content }}
    </div>

  {% endplaceholder %}
{% endblock %}
</div>

{% render_block "js" %}

{% load lastfm_widget %}

<!-- ... -->

{% render_block "analytics" %}

</body>

</html>
